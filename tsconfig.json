{
  "compilerOptions": {
      "lib": ["es6"],
      "types": ["reflect-metadata", "node", "express"],
      "allowJs": true,
      "baseUrl": ".",
      "module": "commonjs",
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "emitDecoratorMetadata": true,
      "noImplicitAny": true,
      "outDir": "./dist/",
      "paths": {
          "*": ["node_modules/*", "vendor/*"]
      },
      "skipLibCheck": true,
      "sourceMap": true,
      "target": "es6",
      "typeRoots": ["node_modules/@types"]
  },
  "exclude": ["clients"]
}
